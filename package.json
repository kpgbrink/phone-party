{
  "name": "phone-party",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "node dist/server/",
    "dev": "vite",
    "dev-node": "tsx watch src/server",
    "dev-all": "concurrently \"npm run dev\" \"npm run dev-node\"",
    "client": "vite",
    "server": "tsx watch src/server",
    "build": "tsc && vite build && tsc -p tsconfig.server.json",
    "build-client": "vite build",
    "build-server": "tsc -p tsconfig.server.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "tsx ./src/shared/GenerateUserAvatarLists.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.12",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fantasy-name-generator": "^2.0.0",
    "phaser": "^3.60.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-code": "^2.0.12",
    "react-router-dom": "^6.16.0",
    "remult": "^0.22.8",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "uniqid": "^5.4.0",
    "use-fit-text": "^2.4.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@types/config": "^3.3.1",
    "@types/express": "^4.17.18",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/simple-peer": "^9.11.6",
    "@types/socket.io": "^3.0.2",
    "@types/uniqid": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "concurrently": "^8.2.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "tsx": "^3.13.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
